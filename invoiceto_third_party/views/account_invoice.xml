<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_invoiceto_account_invoice_form" model="ir.ui.view">
        <field name="name">view.invoiceto.account.invoice.form</field>
        <field name="model">account.invoice</field>
        <field name="inherit_id" ref="account.invoice_tree" />
        <field name="arch" type="xml">
            <field name="partner_id" position="before">
                <field name="partner_shipping_id" string="Cliente" />
            </field>
            <field name="partner_id" position="attributes">
                <attribute name="string">Faturado</attribute>
            </field>
        </field>
     </record>
 </odoo>
