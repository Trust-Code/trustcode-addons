<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_registro_anvisa_product_product_form" model="ir.ui.view">
        <field name="name">view.registro.anvisa.product.product.form</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="product.product_template_only_form_view" />
        <field name="arch" type="xml">
            <field name="barcode" position="after">
                <field name="registro_anvisa" />
                <field name="validade_anvisa" />
                <field name="esterilizacao" />
            </field>
        </field>
    </record>

</odoo>
